{
  "name": "backend",
  "version": "1.0.0",
  "description": "I-Talent team",
  "author": "I-Talent team",
  "license": "MIT",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx concurrently \"nodemon -e yml,js,json src/server.js\" \"yarn -s studio\"",
    "generate": "cd src/database && prisma generate",
    "migrate:dev": "cd src/database && yes | prisma migrate dev",
    "migrate:prod": "cd src/database && yes | prisma migrate deploy",
    "migrate:create": "cd src/database && prisma migrate save",
    "docker:dev": "yarn -s generate && yarn -s migrate:dev && yarn -s seed && yarn -s dev",
    "docker:prod": "yarn -s generate && yarn -s migrate:prod && yarn -s seed && yarn -s start",
    "lint": "./node_modules/.bin/eslint src --cache src ",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "node src/database/seeds",
    "studio": "cd src/database && prisma studio",
    "test": "cross-env NODE_ENV=test jest",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@prisma/client": "^2.29.1",
    "axios": "^0.21.2",
    "body-parser": "^1.19.0",
    "concurrently": "^7.6.0",
    "connect-redis": "^6.0.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.1",
    "fuse.js": "^6.4.6",
    "helmet": "^4.6.0",
    "jest-mock-extended": "^2.0.1",
    "keycloak-connect": "15.0.2",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "morgan": "^1.10.0",
    "redis": "^3.1.2",
    "snyk": "^1.687.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "keycloak-mock": "CDH-Studio/keycloak-mock#v1.0",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^2.29.1",
    "redis-mock": "^0.56.3",
    "supertest": "^6.1.6"
  },
  "snyk": true
}
