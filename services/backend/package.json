{
  "name": "backend",
  "version": "1.0.0",
  "description": "I-Talent team",
  "author": "I-Talent team",
  "license": "MIT",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx concurrently \"nodemon -e yml,js,json src/server.js\" \"yarn -s studio\"",
    "generate": "cd src/database && prisma generate",
    "migrate:dev": "cd src/database && yes | prisma migrate dev",
    "migrate:prod": "cd src/database && yes | prisma migrate deploy",
    "migrate:create": "cd src/database && prisma migrate save",
    "docker:dev": "yarn -s generate && yarn -s migrate:dev && yarn -s seed && yarn -s dev",
    "docker:prod": "yarn -s generate && yarn -s migrate:prod && yarn -s seed && yarn -s start",
    "lint": "./node_modules/.bin/eslint src --cache src ",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "node src/database/seeds",
    "studio": "cd src/database && prisma studio",
    "test": "cross-env NODE_ENV=test jest",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "concurrently": "^7.0.0",
    "connect-redis": "^6.1.1",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "fuse.js": "^6.5.3",
    "helmet": "^5.0.2",
    "jest-mock-extended": "^2.0.4",
    "keycloak-connect": "17.0.0",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "morgan": "^1.10.0",
    "redis": "^4.0.4",
    "snyk": "^1.864.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^6.6.6",
    "jest": "^27.5.1",
    "keycloak-mock": "CDH-Studio/keycloak-mock#v1.0.1",
    "mockdate": "^3.0.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prisma": "3.10.0",
    "redis-mock": "^0.56.3",
    "supertest": "^6.2.2"
  },
  "snyk": true
}
