{
  "name": "backend",
  "version": "1.0.0",
  "description": "I-Talent team",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx concurrently \"nodemon src/server.js\" \"yarn -s studio\"",
    "generate": "cd src/database && prisma generate",
    "migrate": "cd src/database && prisma migrate up --experimental -c",
    "migrate:create": "cd src/database && prisma migrate save --experimental",
    "deploy": "yarn -s migrate && yarn -s seed && yarn -s start",
    "dev:docker": "yarn -s generate && yarn -s migrate && yarn -s seed && yarn -s dev",
    "lint": "./node_modules/.bin/eslint src",
    "seed": "node src/database/seeds",
    "studio": "cd src/database && prisma studio --experimental",
    "test": "jest"
  },
  "author": "I-Talent team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "2.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.5.0",
    "fuse.js": "^6.0.1",
    "jest": "^26.0.1",
    "keycloak-connect": "^10.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
