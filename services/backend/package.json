{
  "name": "backend",
  "version": "1.0.0",
  "description": "I-Talent team",
  "author": "I-Talent team",
  "license": "MIT",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx concurrently \"nodemon -e yml,js,json src/server.js\" \"yarn -s studio\"",
    "generate": "cd src/database && prisma generate",
    "migrate": "cd src/database && prisma migrate up --experimental -c",
    "migrate:create": "cd src/database && prisma migrate save --experimental",
    "docker:dev": "yarn -s generate && yarn -s migrate && yarn -s seed && yarn -s dev",
    "docker:prod": "yarn -s generate && yarn -s migrate && yarn -s seed && yarn -s start",
    "lint": "./node_modules/.bin/eslint src",
    "seed": "node src/database/seeds",
    "studio": "cd src/database && prisma studio",
    "test": "cross-env NODE_ENV=test jest",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@prisma/client": "2.11.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "connect-redis": "^5.1.0",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "fuse.js": "^6.4.4",
    "helmet": "4.6.0",
    "keycloak-connect": "13.0.0",
    "lodash": "^4.17.21",
    "moment": "2.29.1",
    "morgan": "^1.10.0",
    "redis": "^3.1.0",
    "snyk": "1.602.0",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "4.1.6",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.11.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "faker": "^5.1.0",
    "jest": "26.6.3",
    "keycloak-mock": "BenJeau/keycloak-mock#feat-build",
    "mockdate": "3.0.5",
    "nodemon": "2.0.7",
    "prettier": "2.3.0",
    "redis-mock": "^0.56.3",
    "supertest": "6.0.1"
  },
  "snyk": true
}
