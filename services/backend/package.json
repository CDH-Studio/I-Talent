{
  "name": "backend",
  "version": "1.0.0",
  "description": "Upskill backend",
  "main": "server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate up --experimental -c",
    "migrate:create": "prisma migrate save --experimental",
    "deploy": "yarn -s migrate && yarn -s seed && yarn -s start",
    "dev:docker": "yarn -s generate && yarn -s migrate && yarn -s seed && yarn -s dev",
    "test": "jest --config ./tests/jest.config.json",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src",
    "seed": "node prisma/seed"
  },
  "author": "Upskill team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "2.0.0-beta.8",
    "@sentry/node": "5.15.5",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fuse.js": "^6.0.1",
    "jest": "^26.0.1",
    "keycloak-connect": "^10.0.2",
    "moment": "^2.26.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.8",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sequelize-mock": "^0.10.2"
  }
}
