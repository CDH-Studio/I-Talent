{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "react-env && craco start",
    "prestart": "react-env --dest ./build",
    "start": "serve -s build",
    "build": "NODE_ENV=production craco build",
    "test": "craco test",
    "lint": "prettier --check . && ./node_modules/.bin/eslint --cache",
    "format": "prettier --write .",
    "i18n:validate": "node ./src/i18n/validation",
    "i18n:cleanup": "node ./src/i18n/cleanUpUnusedKeys",
    "i18n:alphabetize": "node ./src/i18n/alphabetizeKeys",
    "i18n:export": "node ./src/i18n/exportTransToCSV",
    "analyze": "ANALYZE_BUILD=true yarn build",
    "postinstall": "cd ../../ && husky install services/frontend/.husky"
  },
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@beam-australia/react-env": "3.1.1",
    "@craco/craco": "6.1.2",
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "4.11.4",
    "@material-ui/pickers": "3.3.10",
    "@material-ui/styles": "4.11.4",
    "@react-keycloak/web": "3.4.0",
    "@reduxjs/toolkit": "1.5.1",
    "antd": "4.16.0",
    "axios": "0.21.1",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-lodash": "3.3.4",
    "chart.js": "3.3.0",
    "craco-less": "1.17.1",
    "dayjs": "^1.10.4",
    "keycloak-js": "13.0.1",
    "lodash": "4.17.21",
    "lodash-webpack-plugin": "0.11.6",
    "prop-types": "15.7.2",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-chartjs-2": "3.0.3",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-highlight-words": "0.17.0",
    "react-intl": "5.18.1",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.1.0",
    "redux-persist": "6.0.0",
    "serve": "11.3.2",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^6.0.0",
    "json2csv": "5.0.6",
    "prettier": "2.3.0",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  }
}
