{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "react-env && craco start",
    "prestart": "react-env --dest ./build",
    "start": "serve -s build",
    "build": "NODE_ENV=production craco build",
    "test": "craco test",
    "i18n:check": "node ./src/i18n/check",
    "i18n:cleanup": "node ./src/i18n/cleanup",
    "analyze": "ANALYZE_BUILD=true yarn build",
    "lint": "./node_modules/.bin/eslint --cache --format codeframe --ext jsx,js src",
    "prettify": "prettier --write ."
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@beam-australia/react-env": "^2.1.2",
    "@craco/craco": "^5.5.0",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.5.0",
    "antd": "^4.10.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "craco-less": "^1.17.1",
    "dayjs": "^1.10.2",
    "history": "^5.0.0",
    "keycloak-js": "^12.0.1",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-app-polyfill": "^2.0.0",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.16.0",
    "react-intl": "^5.10.14",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "serve": "^11.3.2",
    "validator": "^13.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lodash-webpack-plugin": "^0.11.6",
    "prettier": "^2.2.1",
    "webpack": "^5.12.1",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
